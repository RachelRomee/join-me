<h1>Invitations <%= link_to "+", new_listing_path, class: "btn-floating btn-large waves-effect waves-light red", style: "float:right" %></h1>



<%= link_to listing.user.name, root_path %>


<br>

<%= render @listings %>


<% @listings.each do |listing| %>
  <div class="card">
    <p><%= link_to listing.title, listing %></p>
    <p><%= listing.date %></p>
    <p><%= listing.city %></p>
    <p><%= listing.content %></p>
    <p><%= listing.user.name %></p>
  </div>
<% end %>
