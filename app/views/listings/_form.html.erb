
<div class="row">
<div class="col s12 m8 offset-m2 center">


<%= form_for @listing do |f| %>

  <div class="input-field">
    <%= f.label :title %>
    <%= f.text_field :title, autofocus: true %>
  </div>

  <div class="input-field">
  <%= f.label :city %>
  <%= f.text_field :city, autofocus: true %>
  </div>

  <br/>


  <div class="input-field">
  <%= f.label :description  %>
  <%= f.text_area :content, autofocus: true %>
  </div>
<br />
  <%= f.submit class: "btn waves-effect waves-light" %>
  <%= link_to "Back", :back, :class => "btn waves-effect waves-light" %>
<% end %>
