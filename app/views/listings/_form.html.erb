
<div class="row">
<div class="col s12 m6 offset-m3 center">


<%= form_for @listing do |f| %>

  <div class="input-field">
    <%= f.label :title %>
    <%= f.text_field :title, autofocus: true, :placeholder => "Title" %>
  </div>


  <br/>
  <div class="input-field">
  <%= f.label :city, :placeholder => "What city will you go to?" %>
  <%= f.text_field :city, autofocus: true %>
  </div>


  <%= f.submit class: "btn waves-effect waves-light" %>
  <%= link_to "Back", :back, :class => "btn waves-effect waves-light" %>
<% end %>
