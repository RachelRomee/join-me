<div class="row">
  <div class="col s12 m7">
    <div class="card">
      <div class="card-image">
        <%= image_tag listing.user.photo %>
        <span class="card-title"><%= listing.title %></span>
      </div>
      <div class="card-content">
        <p><%= listing.city %></p>
      </div>
      <div class="card-action">
        <a class="waves-effect waves-light btn">button</a>
      </div>
    </div>
  </div>
</div>

